{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": false,
        "noImplicitThis": false,
        "skipLibCheck": true,
        "noUnusedLocals": false,
        "importHelpers": true,
        "removeComments": false,
        "noLib": false,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "useUnknownInCatchVariables": false,
        "target": "es6",
        "sourceMap": true,
        "allowJs": false,
        "strict": true,
        "strictNullChecks": false,
        "types": [
            "node",
            "jest",
            "@types/jest"
        ]
    }
}